extern func print_int(int) void;
extern func print_string(string) void;

package Breakstmt {
  func main() int {
    var i, j int;

    for (i = 0; i < 10; i = i + 1) {
      if (i == 5) {
        break;
      }
      print_int(i);
    }

    i = 0;

    while (i < 10) {
      if (i == 5) {
        break;
      }
      print_int(i);
      i = i + 1;
    }

    return (0);
  }
}