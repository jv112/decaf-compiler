extern func print_int(int) void;
extern func print_string(string) void;

package Continuestmt {
  func main() int {
    var i, j int;

    for (i = 0; i < 10; i = i + 1) {
      if (i % 2 == 0) {
        continue;
      }
      print_int(i);
    }

